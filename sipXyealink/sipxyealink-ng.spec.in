Name: @PACKAGE@
Version: @VERSION@
Release: @PACKAGE_REVISION@

Summary: VoIP PBX plugin.
License: AGPL
Group: Telcommunications
Vendor: SIPLABS
Packager: SIPLABS <techinfo@siplabs.ru>
Url: http://www.siplabs.ru

Obsoletes: @PACKAGE_OBSOLETE_NAME@
BuildRequires: java-1.7.0-openjdk-devel
BuildRequires: automake
BuildRequires: sipxcommons-ng >= %version
BuildRequires: sipxconfig-ng >= %version

Requires: sipxconfig-ng

Source: %name-%version.tar.gz

Prefix: %_prefix
BuildRoot: %{_tmppath}/%name-%version-root

%description
Configuration support yealink phones

%prep
%setup -q

%build
%configure
make

%install
rm -rf $RPM_BUILD_ROOT
make DESTDIR=$RPM_BUILD_ROOT install

%clean
rm -rf $RPM_BUILD_ROOT

%files
%defattr(644,root,root,755)
%{_sysconfdir}/sipxpbx/yealink/*
%{_datadir}/java/sipXecs/sipXconfig/plugins/sipXyealink.jar

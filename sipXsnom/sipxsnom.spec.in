Name: @PACKAGE@
Version: @VERSION@
Release: @PACKAGE_REVISION@

Summary: VoIP PBX 
License: LGPL
Group: Telcommunications
Vendor: SIPfoundry
Packager: SIPfoundry <sipx-dev@sipfoundry.org>
Url: http://www.sipfoundry.org

Obsoletes: @PACKAGE_OBSOLETE_NAME@
%if %{_vendor} == suse
BuildRequires: java-1_6_0-sun-devel
%else
BuildRequires: java-1.7.0-openjdk-devel
%endif
BuildRequires: automake
BuildRequires: sipxcommons-ng >= %version
BuildRequires: sipxconfig-ng >= %version

Requires: sipxconfig-ng

Source: %name-%version.tar.gz

Prefix: %_prefix
BuildRoot: %{_tmppath}/%name-%version-root

%description
Configuration support snom phones

%prep
%setup -q

%build
%configure
make

%install
rm -rf $RPM_BUILD_ROOT
make DESTDIR=$RPM_BUILD_ROOT install

%clean
rm -rf $RPM_BUILD_ROOT

%files
%defattr(644,root,root,755)
%{_sysconfdir}/sipxpbx/snom/*
%{_sysconfdir}/sipxpbx/snom-m3/*
%{_datadir}/java/sipXecs/sipXconfig/plugins/sipXsnom.jar



